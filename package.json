{
  "name": "bootstrap-demo",
  "version": "1.0.1",
  "homepage": "https://sebgroup.github.io/bootstrap/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --base-href https://sebgroup.github.io/bootstrap/ && copyfiles -f src/404.html dist/bootstrap",
    "prebuild": "rimraf dist/bootstrap && replace __TRAVIS_BUILD_NUMBER__ $TRAVIS_BUILD_NUMBER src/environments/environment.ts && replace __TRAVIS_BUILD_NUMBER__ $TRAVIS_BUILD_NUMBER src/environments/environment.prod.ts",
    "build-dev": "ng build --base-href https://sebgroup.github.io/bootstrap/dev/ --output-path dist/bootstrap/dev && copyfiles -f src/dev/404.html dist/bootstrap/dev",
    "prebuild-dev": "rimraf dist/bootstrap/dev",
    "build-lib": "npm run copy-scss && npm run copy-fonts && npm run compile-css && copyfiles -f CHANGELOG.md LICENSE.md README.md scss/package.json dist/lib",
    "build-sdl": "rimraf dist/bootstrap/sdl && copyfiles -u 2 src/**/*.md dist/bootstrap/sdl/ && gulp build-sdl && copyfiles -f scss/targetindex.json dist/lib/package.json dist/bootstrap/sdl/ && renamer --find \"package\" --replace \"version\" dist/bootstrap/sdl/*.json && copyfiles -f dist/lib/css/bootstrap-all* dist/bootstrap/",
    "prebuild-lib": "rimraf dist/lib",
    "prepublish-lib": "npm run build-lib",
    "publish-lib": "npm publish dist/lib",
    "build-css": "node-sass --importer=node_modules/node-sass-tilde-importer --source-map true --include-path scss scss/bootstrap-prebuild.scss dist/lib/css/bootstrap-prebuild.css",
    "autoprefix-css": "postcss -u autoprefixer -r dist/lib/css/bootstrap-prebuild.css",
    "minify-css": "cleancss -o dist/lib/css/bootstrap-prebuild.min.css dist/lib/css/bootstrap-prebuild.css --source-map",
    "build-all-css": "node-sass --importer=node_modules/node-sass-tilde-importer --source-map true --include-path scss scss/bootstrap-prebuild-all.scss dist/lib/css/bootstrap-prebuild-all.css",
    "autoprefix-all-css": "postcss -u autoprefixer -r dist/lib/css/bootstrap-prebuild-all.css",
    "minify-all-css": "cleancss -o dist/lib/css/bootstrap-prebuild-all.min.css dist/lib/css/bootstrap-prebuild-all.css --source-map",
    "rename-css": "renamer --find \"p-prebuild\" --replace \"p\" dist/lib/css/*",
    "compile-css": "npm run build-css && npm run autoprefix-css && npm run minify-css && npm run build-all-css && npm run autoprefix-all-css && npm run minify-all-css && npm run rename-css",
    "copy-scss": "copyfiles -u 1 'scss/**/*.scss' -e 'scss/**/*-prebuild*.scss' dist/lib/scss",
    "copy-fonts": "copyfiles -u 2 scss/fonts/* dist/lib/fonts",
    "test": "ng test",
    "test-ci": "ng test --watch=false --progress --browsers=ChromeHeadlessNoSandbox",
    "visualtest:ci": "start-server-and-test start http://localhost:4200 visualtest",
    "visualtest": "backstop test",
    "visualtest:approve": "backstop approve",
    "add-fa-config": "npm config set '@fortawesome:registry' https://npm.fontawesome.com/ && npm config set '//npm.fontawesome.com/:_authToken' $FA_TOKEN",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "commit": "git-cz",
    "travis-deploy-once": "travis-deploy-once --pro",
    "semantic-release": "semantic-release && npm run build && npm run build-sdl"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit"
    }
  },
  "release": {
    "pkgRoot": "dist/lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.5",
    "@angular/common": "^7.2.5",
    "@angular/compiler": "^7.2.5",
    "@angular/core": "^7.2.5",
    "@angular/forms": "^7.2.5",
    "@angular/http": "^7.2.5",
    "@angular/platform-browser": "^7.2.5",
    "@angular/platform-browser-dynamic": "^7.2.5",
    "@angular/router": "^7.2.5",
    "@fortawesome/angular-fontawesome": "^0.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/pro-light-svg-icons": "^5.7.2",
    "@ng-bootstrap/ng-bootstrap": "^4.1.1",
    "@sebgroup/fonts": "^1.0.0",
    "angular-exemplify": "^3.1.3",
    "core-js": "^2.6.5",
    "ngx-toastr": "^9.1.2",
    "rxjs": "^6.4.0",
    "scrollingelement": "^1.5.2",
    "swe-validation": "^1.0.1",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/cli": "^7.3.2",
    "@angular/compiler-cli": "^7.2.5",
    "@angular/language-service": "^7.2.5",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@fortawesome/fontawesome-pro": "^5.7.2",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^8.10.40",
    "autoprefixer": "^9.4.8",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "backstopjs": "^3.8.8",
    "body-scroll-lock": "^2.6.1",
    "bootstrap": "^4.3.1",
    "clean-css-cli": "^4.2.1",
    "codelyzer": "~4.2.1",
    "commitizen": "^2.10.1",
    "copyfiles": "^2.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eyes.selenium": "0.0.83",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.0",
    "gulp-json-format": "^2.0.0",
    "gulp-jsoncombine": "^1.0.4",
    "gulp-markdown-to-json": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mobile-detect": "^1.4.3",
    "node-sass-tilde-importer": "^1.0.2",
    "postcss-cli": "^6.1.1",
    "prismjs": "^1.15.0",
    "protractor": "^5.4.2",
    "raw-loader": "^0.5.1",
    "renamer": "^1.1.1",
    "replace": "^1.0.1",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "start-server-and-test": "^1.7.11",
    "travis-deploy-once": "^5.0.11",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebgroup/bootstrap.git"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
